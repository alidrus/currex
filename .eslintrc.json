{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": ["error", 4],
        "no-console": ["error", {"allow": ["log", "warn", "error", "dir"]}],
        "no-param-reassign": ["error", { "props": false }],
        "prefer-template": "off" 
    }
}
